{
  "name": "test-driven-carousel",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watch:coverage": "jest --watchAll --coverage --coverageReporters=text",
    "format": "prettier --single-quote --trailing-comma=es5 --write '**/*.js'",
    "lint:js": "prettier --single-quote --trailing-comma=es5 --write '**/*.js' && eslint --fix .",
    "lint:css": "stylelint '**/*.js'",
    "lint": "npm run lint:js && npm run lint:css",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack-dev-server --config webpack.config.js --open 'Google Chrome'"
